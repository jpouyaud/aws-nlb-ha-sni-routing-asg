region: {{ ws.ec2.region }}
api_endpoint: http://127.0.0.1:8080/api
sync_interval_in_seconds: 1
cloud_provider: AWS
upstreams:
  - name: app_one
    autoscaling_group: ngx-oss-one-autoscaling
    port: 443
    kind: stream
    max_conns: 0
    max_fails: 1
    fail_timeout: 10s
    slow_start: 0s
  - name: app_two
    autoscaling_group: ngx-oss-two-autoscaling
    port: 443
    kind: stream
    max_conns: 0
    max_fails: 1
    fail_timeout: 10s
    slow_start: 0s
